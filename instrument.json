{
    "title": "Instrument",
    "description": "An instrument used by a Participant in a Sonic Multiplicities Session",
    "type": "object",
    "required": [
        "make",
        "model"
    ],
    "properties": {
        "category": {
            "type": "string",
            "default": "Acoustic Guitar"
        },
        "make": {
            "type": "string",
            "default": "Gibson"
        },
        "model": {
            "type": "string",
            "default": "Les Paul or whatever"
        },
        "tuning": {
            "type": "string",
            "default": "standard"
        }
    }
}
